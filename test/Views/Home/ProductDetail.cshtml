    function detailcart(productId) {
        if ($("#logstatus").val() == "0") {
            $("#itemModal").modal("show");
            $.ajax({
                url: "login?ReturnUrl=Section",
                success: function (data) {
                    $("#itemModal").html(data);
                }
            });
        } else {
            if ('@ispush'==''){
                $.getJSON("AddToCart", { ProductId: productId, IsPush: null }, function (res) {
                    if (res == true) {
                        alert("加入購物車成功!!");
                    } else {
                        //alert("商品已在購物車!!");
                    }
                });
                }else{
                $.getJSON("AddToCart", { ProductId: productId, IsPush: '@ispush' }, function (res) {
                    if (res == true) {
                        alert("加入購物車成功!!");
                    } else {
                        //alert("商品已在購物車!!");
                    }
                });
            }
                
        }
    }
    function selectreply(obj) {
        try {
            tempobj.css("background-color", "#FFFFFF");
        } catch (exception) { }
        obj.css("background-color", "#f6fe48");
        $("#replyto").html(obj.data("name"));
        $("#replytoid").val(obj.data("id"));
        $("#replytopid").val(obj.data("productid"));
        //alert($("#replyto").html() + ", " + $("#replytoid").val() + ", " + $("#replytopid").val());
        tempobj = obj;
    }
    function replyto() {
        var rid = $("#replytoid").val();
        var pid = $("#MainProductId").html();
        var replyContext = $("#replyContext").val();
        if (rid != null && rid != "" && replyContext != "") {
            $.ajax({
                url: 'replyto',
                data: { Id: rid, ProductId: pid, replyContext: replyContext },
                type: "Get",

                success: function (res) {
                    window.location.href = "/home/productdetail?ProductId=" + pid;
                }
            });
        } else {
            alert("請確認收件者及內文!!");
        }
    }
    function reply() {
        var pid = $("#MainProductId").html();
        var replyContext = $("#replyContext").val();
        if (replyContext != "") {
            $.ajax({
                url: 'reply',
                data: { ProductId: pid, replyContext: replyContext },
                type: "Get",

                success: function (res) {
                    window.location.href = "/home/productdetail?ProductId=" + pid;
                }
            });
        } else {
            alert("請確認內文!!");
        }
    }
    </script>
