            var name = document.getElementById('ctl00_ContentPlaceHolder1_ProductSearchControl1_txtKeyWord').value;
            $.ajax({
                url: 'SectionPartial',
                data: { page: 1, id: '@id', name: name },
                type: "Get",

                success: function (res) {
                    $("#grid").html(res);
                    menu_ok('@id', name);
                },

                error: function (xhr, ajaxOptions, thrownError) {
                    alert(xhr.status);
                    alert(thrownError);
                }
            });
