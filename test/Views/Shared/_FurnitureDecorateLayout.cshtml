<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/CSS/Content")
    <style>
        body {
            padding-top: 0px;
        }
    </style>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    @RenderBody()
    @Scripts.Render("~/jQuery/ThreeJs/three.min.js")
    @Scripts.Render("~/bundles/3DTest")
    @RenderSection("scripts", required: false)
</body>
</html>
